	<queryString>
		<![CDATA[SELECT 	CASE R.CRRSD_MODALIDAD
		WHEN 0 THEN 'PERMANENTE'
		WHEN 1 THEN 'TEMPORAL'
	END CRRSD_MODALIDAD,
	R.CRRSD_FECHA_INICIO,
	R.CRRSD_FECHA_CADUCIDAD,
	COALESCE(P.CRPER_NOMBRES) ||' ' ||COALESCE(P.CRPER_APELLIDO_PATERNO) CRPER_NOMBRES,
	P.CRPER_NUM_DOC_IDENTIFIC,
	N.CGNCN_NACIONALIDAD
FROM CGG_RES_RESIDENCIA R
INNER JOIN CGG_RES_PERSONA P ON P.CRPER_CODIGO=R.CRPER_CODIGO
INNER JOIN CGG_NACIONALIDAD N ON N.CGNCN_CODIGO=P.CGNCN_CODIGO
WHERE R.CRRSD_VIGENTE='t'
AND R.CRRSD_MODALIDAD=$P{P_CRRSD_MODALIDAD}
AND R.CRRSD_FECHA_INICIO BETWEEN $P{P_FECHA_INICIAL} AND $P{P_FECHA_FINAL}]]>
	</queryString>
