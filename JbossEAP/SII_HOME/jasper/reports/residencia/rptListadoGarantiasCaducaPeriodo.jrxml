	<queryString>
		<![CDATA[SELECT
	DG.CRDPT_OBSERVACION,
	DG.CRDPT_VALOR_DEPOSITO,
	DG.CRDPT_FECHA_DEPOSITO,
	DG.CRDPT_FECHA_ENTREGA,
	DG.CRDPT_FECHA_CADUCIDAD,
	CASE DG.CRDPT_TIPO
	WHEN 0 THEN 'DEPOSITO'
	WHEN 1 THEN 'CHEQUE'
	WHEN 2 THEN 'POLIZA'
	WHEN 3 THEN 'OTROS'
	END CRDPT_TIPO,
	(SELECT PJ.CRPJR_RAZON_SOCIAL
		FROM SII.CGG_RES_PERSONA_JURIDICA PJ
		WHERE DG.CRPJR_CODIGO = PJ.CRPJR_CODIGO) CRPJR_RAZON_SOCIAL,
	CPJ.CRCPJ_NUMERO_CUENTA
FROM SII.CGG_RES_DEPOSITO_GARANTIA DG
LEFT JOIN SII.CGG_RES_CUENTA_PERSONAJURIDICA CPJ ON(CPJ.CRCPJ_CODIGO=DG.CRCPJ_CODIGO)
WHERE DG.CRDPT_FECHA_CADUCIDAD BETWEEN $P{P_FECHA_INICIAL} AND $P{P_FECHA_FINAL}
ORDER BY DG.CRDPT_FECHA_CADUCIDAD ASC]]>
	</queryString>
