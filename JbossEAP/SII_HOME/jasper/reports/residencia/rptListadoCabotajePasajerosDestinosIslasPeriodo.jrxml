	<parameter name="P_FECHA_INICIAL" class="java.util.Date"/>
	<parameter name="P_FECHA_FINAL" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
	P.CRPER_NUM_DOC_IDENTIFIC,
	COALESCE(P.CRPER_NOMBRES,'')||' '||COALESCE(P.CRPER_APELLIDO_PATERNO,'') CRPER_NOMBRES,
	(SELECT COALESCE(C.CCTN_NOMBRE,' ')
		FROM CGG_RES_MUELLE M, CGG_CANTON C
		WHERE M.CRMLE_CODIGO=MI.CRMLE_CODIGO
		AND M.CCTN_CODIGO=C.CCTN_CODIGO) ISLA_ORIGEN,
	(SELECT COALESCE(C.CCTN_NOMBRE,' ')
		FROM CGG_RES_MUELLE M,CGG_CANTON C
		WHERE M.CRMLE_CODIGO=MI.CGG_CRMLE_CODIGO
		AND M.CCTN_CODIGO=C.CCTN_CODIGO) ISLA_DESTINO,
	MI.CRMVI_FECHA_VIAJE,
	E.CREMB_NOMBRE


FROM CGG_RES_MOVILIDAD_INTERNA MI
INNER JOIN CGG_RES_PERSONA P ON (P.CRPER_CODIGO=MI.CRPER_CODIGO)
INNER JOIN CGG_RES_EMBARCACION E ON (E.CREMB_CODIGO = MI.CREMB_CODIGO)
WHERE DATE(MI.CRMVI_FECHA_VIAJE) BETWEEN $P{P_FECHA_INICIAL} AND $P{P_FECHA_FINAL}
ORDER BY  MI.CRMVI_FECHA_VIAJE,ISLA_DESTINO]]>
	</queryString>
