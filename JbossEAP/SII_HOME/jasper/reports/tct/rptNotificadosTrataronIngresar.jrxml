		<![CDATA[SELECT
TO_CHAR(INF.CTIGF_FECHA_INTENTO::DATE,'YYYY') ANIO,
TO_CHAR(INF.CTIGF_FECHA_INTENTO::DATE,'TMMONTH') MES,
COUNT(NTF.CRNOT_CODIGO)
FROM SII.CGG_RES_NOTIFICACION NTF
INNER JOIN SII.CGG_RES_PERSONA PER ON (NTF.CRPER_CODIGO=PER.CRPER_CODIGO)
INNER JOIN SII.CGG_TCT_INGRESO_FALLIDO INF ON(INF.CRPER_CODIGO=PER.CRPER_CODIGO)
WHERE NTF.CRNOT_ESTADO
AND INF.CTIGF_FECHA_INTENTO::DATE BETWEEN $P{P_FECHA_INICIAL} AND $P{P_FECHA_FINAL}
GROUP BY ANIO, MES
ORDER BY ANIO ASC]]>
