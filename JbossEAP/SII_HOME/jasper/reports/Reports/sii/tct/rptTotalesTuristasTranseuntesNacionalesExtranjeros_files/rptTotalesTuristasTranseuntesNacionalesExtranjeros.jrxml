	<parameter name="P_FECHA_INICIAL" class="java.util.Date"/>
	<parameter name="P_FECHA_FINAL" class="java.util.Date"/>
	<parameter name="P_CUSU_CODIGO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	CASE MOV.CRMOV_TIPO_OPERACION
	WHEN 0 THEN 'ENTRADA'
	WHEN 1 THEN 'SALIDA'
	END CRMOV_TIPO_OPERACION,

	(SELECT SOL.CRTST_DESCRIPCION FROM SII.CGG_RES_TIPO_SOLICITUD_TRAMITE SOL WHERE SOL.CRTST_CODIGO = F_TIPO_SOLICITUD_PADRE(RES.CRTST_CODIGO) ) CRTST_DESCRIPCION,
	CASE PER.CGNCN_CODIGO
	WHEN (SELECT UPPER(TRIM(CGCNF_VALOR_CADENA)) FROM SII.CGG_CONFIGURACION WHERE CGCNF_CODIGO = 'CONF36') THEN 'Nacional'
	ELSE 'Extranjero' END CGNCN_NACIONALIDAD,
	SII.F_CGG_RES_PERSONA_SELECT_NOMBRES(USU.CRPER_CODIGO) CRPER_NOMBRE,
	CRMOV_NUMERO_VUELO,
	COUNT(MOV.CRMOV_CODIGO)
FROM SII.CGG_RES_MOVILIDAD MOV
INNER JOIN SII.CGG_RES_RESIDENCIA RES ON (RES.CRRSD_CODIGO=MOV.CRRSD_CODIGO)
INNER JOIN SII.CGG_RES_PERSONA PER ON (PER.CRPER_CODIGO=RES.CRPER_CODIGO)
INNER JOIN SII.CGG_USUARIO USU ON(USU.CUSU_NOMBRE_USUARIO=MOV.CRMOV_USUARIO_INSERT)
WHERE MOV.CRMOV_ESTADO=TRUE
AND DATE(MOV.CRMOV_FECHA_VIAJE) BETWEEN $P{P_FECHA_INICIAL} AND $P{P_FECHA_FINAL}
AND (SII.F_TIPO_SOLICITUD_PADRE(RES.CRTST_CODIGO) IN (SELECT UPPER(TRIM(CGCNF_VALOR_CADENA)) FROM SII.CGG_CONFIGURACION WHERE CGCNF_CODIGO IN ('05','06')))
AND USU.CUSU_CODIGO = $P{P_CUSU_CODIGO}

GROUP BY CRMOV_TIPO_OPERACION,CGNCN_NACIONALIDAD,CRTST_DESCRIPCION,CRMOV_NUMERO_VUELO,CRPER_NOMBRE
ORDER BY CGNCN_NACIONALIDAD,CRPER_NOMBRE,CRMOV_NUMERO_VUELO]]>
	</queryString>
