	<queryString>
		<![CDATA[SELECT 	E.CKESP_NOMBRE,
	E.CKESP_TIPO,
	T.CKTRN_CANTIDAD,
	T.CKTRN_CANTIDAD_DISPONIBLE,
	T.CKTRN_PRECIO_UNITARIO,
	T.CKTRN_SERIE_INICIO,
	T.CKTRN_SERIE_FIN,
	P.CKPVT_NOMBRE,
	C.CCTN_NOMBRE,
	T.CKTRN_FECHA_RECEPCION
FROM CGG_KDX_PUNTO_VENTA P
INNER JOIN CGG_CANTON C ON C.CCTN_CODIGO= P.CCTN_CODIGO
INNER JOIN CGG_KDX_TRANSFERENCIA T ON T.CKPVT_CODIGO=P.CKPVT_CODIGO
INNER JOIN CGG_KDX_ESPECIE E ON E.CKESP_CODIGO=T.CKESP_CODIGO
WHERE C.CCTN_CODIGO=$P{P_CCTN_CODIGO}
AND E.CKESP_TIPO = 0
ORDER BY C.CCTN_NOMBRE,CKESP_NOMBRE,T.CKTRN_FECHA_RECEPCION DESC]]>
	</queryString>
