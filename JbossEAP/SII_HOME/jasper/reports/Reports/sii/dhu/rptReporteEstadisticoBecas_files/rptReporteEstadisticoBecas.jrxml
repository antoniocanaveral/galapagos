	<queryString>
		<![CDATA[SELECT
TO_CHAR(BEC.CDBEC_FECHA_INICIO,'YYYY') ANIO,
TBE.CDTBC_NOMBRE,
NES.CGNES_DESCRIPCION,
COUNT(BEC.CDBEC_CODIGO)AS BECA
FROM SII.CGG_DHU_BECA BEC
INNER JOIN SII.CGG_DHU_TIPO_BECA TBE ON (TBE.CDTBC_CODIGO=BEC.CDTBC_CODIGO)
LEFT JOIN SII.CGG_ESPECIALIDAD ESP ON(ESP.CGESP_CODIGO =BEC.CGESP_CODIGO)
LEFT JOIN SII.CGG_INSTITUCION_NIVEL INI ON(INI.CGIED_CODIGO=BEC.CGIED_CODIGO)
LEFT JOIN SII.CGG_NIVEL_ESTUDIO NES ON(NES.CGNES_CODIGO=INI.CGNES_CODIGO)
WHERE BEC.CDBEC_ESTADO
AND BEC.CDBEC_FECHA_INICIO::DATE BETWEEN $P{P_FECHA_INICIAL}::DATE AND $P{P_FECHA_FINAL}::DATE
AND TBE.CDTBC_CODIGO=$P{P_CDTBC_CODIGO}
GROUP BY ANIO, CDTBC_NOMBRE,NES.CGNES_DESCRIPCION
ORDER BY ANIO, CDTBC_NOMBRE DESC]]>
	</queryString>
