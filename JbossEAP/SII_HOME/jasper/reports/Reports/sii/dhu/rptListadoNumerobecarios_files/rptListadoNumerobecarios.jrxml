	<queryString>
		<![CDATA[SELECT
    ( CRPER_NOMBRES||''||CRPER_APELLIDO_PATERNO||''||CRPER_APELLIDO_MATERNO) AS CRPER_NOMBRES,
     PER.CRPER_NUM_DOC_IDENTIFIC,
     CDTBC_NOMBRE
FROM
     SII.CGG_DHU_TIPO_BECA TBC INNER JOIN SII.CGG_DHU_BECA BCA ON TBC.CDTBC_CODIGO = BCA.CDTBC_CODIGO
INNER JOIN SII.CGG_RES_PERSONA PER ON PER.CRPER_CODIGO=BCA.CRPER_CODIGO
WHERE
     TBC.CDTBC_CODIGO=$P{P_CDTBC_CODIGO}
GROUP BY
     CDTBC_NOMBRE,CRPER_NOMBRES,CRPER_APELLIDO_PATERNO,CRPER_APELLIDO_MATERNO,PER.CRPER_NUM_DOC_IDENTIFIC]]>
	</queryString>
