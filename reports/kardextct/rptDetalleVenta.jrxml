	<queryString>
		<![CDATA[SELECT
SUM(VDT.CKVDT_CANTIDAD) CKVDT_CANTIDAD,
'Especies '||ESP.CKESP_NOMBRE DETALLE,
VDT.CKVDT_SERIE,
VDT.CKVDT_PRECIO_UNITARIO,
SUM(VDT.CKVDT_TOTAL) CKVDT_TOTAL
FROM SII.CGG_KDX_VENTA_DETALLE VDT
INNER JOIN SII.CGG_KDX_ESPECIE ESP ON(ESP.CKESP_CODIGO=VDT.CKESP_CODIGO)
WHERE VDT.CKVDT_ESTADO
AND VDT.CKVNT_CODIGO=$P{P_CKVNT_CODIGO}
GROUP BY DETALLE,CKVDT_SERIE,CKVDT_PRECIO_UNITARIO,CKVDT_TOTAL]]>
